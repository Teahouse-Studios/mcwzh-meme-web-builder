    "core-js": "^3.17.3",
    "vue": "^2.6.14",
    "vue-cli-plugin-i18n": "^2.3.1",
    "vue-i18n": "^8.26.1",
    "vuetify": "^2.6.0",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vitejs/plugin-legacy": "^1.4.4",
    "aws-sdk": "^2.912.0",
