  "dependencies": {
    "@mdi/js": "^5.8.55",
    "axios": "^0.21.1",
    "core-js": "^3.8.1",
    "vue": "^2.6.11",
    "vue-cli-plugin-i18n": "^1.0.1",
    "vue-i18n": "^8.22.2",
    "vuetify": "^2.4.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.10",
    "@vue/cli-plugin-eslint": "~4.5.9",
    "@vue/cli-service": "~4.5.9",
    "ali-oss": "^6.12.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.4.0",
    "glob": "^7.1.6",
    "node-ssh": "^11.1.1",
